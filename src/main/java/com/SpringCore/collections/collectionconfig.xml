		<property name="phones">
			<list>
				<value>8294921058</value>
				<value>9308115676</value>
				<null></null>

			</list>
		</property>

		<property name="address">
			<set>
				<value>naubatpur</value>
				<value>patna</value>
				<value>delhi</value>
			</set>
		</property>

		<property name="courses">
			<map>
				<entry key="java" value="2month" />
				<entry key="c++" value="4month" />
				<entry key="c" value="1month" />
			</map>
		</property>
	</bean>


</beans>
 
 